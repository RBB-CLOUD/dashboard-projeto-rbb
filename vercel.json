{
  "version": 2,
  "routes": [
    {
      "src": "^/api/(.*)",
      "dest": "https://supabase-realtime-worker.onrender.com/api/$1"
    },
    {
      "src": "^/site/(.*)",
      "dest": "/site/$1"
    },
    {
      "src": "^/$",
      "dest": "/site/dashboard.html"
    },
    {
      "src": "^/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
